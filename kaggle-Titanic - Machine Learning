    "#preprocess\n",
    "train_data = train_data.replace([\"female\",\"male\"],[0,1])\n",
    "train_data = train_data.replace([\"S\",\"C\",\"Q\"],[0,1,2])\n",
    "train_data = train_data.fillna(0)\n",
    "train_data.head()"
   ]
